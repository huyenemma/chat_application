<h1>Message!</h1>
<form method="POST">
  <input type="text" name="sender" placeholder="Your Name" required/>
  <textarea name="message" placeholder="Your Message" required></textarea>
  <input type="submit" value="Send!" />
</form>

<% it.messages.forEach(msg => { %>
  <div>
    <%= msg.sender %>: <%= msg.message %>
  </div>
<% }); %>